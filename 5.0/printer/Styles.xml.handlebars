<StyleList xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://stollsoft.de/iSign/v2/Content/Forms/Styles">
{{#*inline "commonFields"}}
<field key="ml">
  <names>
    <name>Mehrsprachig</name>
  </names>
    <content_class type="TextChoose">
    <text key="0">Kein</text>
    <text key="1">Nur Monitor</text>
{{#if enable_app}}
    <text key="2">Nur App</text>
    <text key="3">Monitor und App</text>
{{/if}}
  </content_class>
</field>
<field key="mode">
  <names>
    <name>Modus</name>
  </names>
    <content_class type="TextChoose">
    <text key="show">Nummer anzeigen</text>
    <text key="print">Nummer drucken</text>
  </content_class>
</field>
{{#if enable_app}}
<field key="qr">
  <names>
    <name>QR Code</name>
  </names>
    <content_class type="TextChoose">
    <text key="0">Kein</text>
    <text key="1">Nur Monitor</text>
    <text key="2">Nur Ticket</text>
    <text key="3">Monitor und Ticket</text>
  </content_class>
</field>
{{/if}}
<field key="wt">
  <names>
    <name>Wartezeit</name>
  </names>
    <content_class type="TextChoose">
    <text key="0">Verbergen</text>
    <text key="1">Nur Monitor</text>
{{#if enable_app}}
    <text key="2">Nur App</text>
    <text key="3">Monitor und App</text>
{{/if}}
  </content_class>
</field>
<field key="ar">
  <names>
    <name>Hinweispfeil</name>
  </names>
  <content_class type="TextChoose">
    <text key="r">rechts</text>
    <text key="d">unten</text>
  </content_class>
</field>
<field key="tracking_id">
  <names>
    <name>Tracking-ID</name>
  </names>
  <content_class type="TextFree" />
</field>
{{/inline}}

{{#*inline "timeoutField"}}
<field key="it">
  <names>
    <name>Leerlauf Timeout</name>
  </names>
  <content_class type="TextChoose">
    <text key="8">8 Sekunden</text>
    <text key="10">10 Sekunden</text>
    <text key="15">15 Sekunden</text>
    <text key="20">20 Sekunden</text>
    <text key="30">30 Sekunden</text>
    <text key="40">40 Sekunden</text>
    <text key="60">60 Sekunden</text>
  </content_class>
</field>
{{/inline}}

{{#*inline "printerFields"}}
{{#range 1 10}}
<field key="text{{this}}">
  <names>
    <name>Name {{this}}</name>
  </names>
  <content_class type="TextFree" />
</field>
<field key="queueid{{this}}">
  <names>
    <name>Warteschlange {{this}}</name>
  </names>
  <content_class type="VocatisQueue"  />
</field>
<field key="catid{{this}}">
    <names>
        <name>Kategorie {{this}}</name>
    </names>
    <content_class type="VocatisCategories"/>
</field>
{{/range}}
{{/inline}}

{{#*inline "appointmentFields"}}
<field key="ns">
  <names>
    <name>Wartenummern Quelle</name>
  </names>
  <content_class type="TextChoose">
    <text key="">Nach Vocatis Nummernschema</text>
    <text key="user">Aus Feld 'Number'</text>
    <text key="T3">T + letze drei aus Feld 'Number'</text>
    <text key="T4">T + letze vier aus Feld 'Number'</text>
    <text key="T5">T + letze fünf aus Feld 'Number'</text>
  </content_class>
</field>
{{#range 1 13}}
<field key="ap_pn{{this}}">
  <names>
    <name>Terminplan {{this}}</name>
  </names>
  <content_class type="TextFree" />
</field>
<field key="ap_qi{{this}}">
  <names>
    <name>Warteschlange Termin {{this}}</name>
  </names>
  <content_class type="VocatisQueue"  />
</field>
{{/range}}
<field key="catid_apt">
  <names>
    <name>Kategorie Termin</name>
  </names>
  <content_class type="VocatisCategories"/>
</field>
<field key="pp">
  <names>
    <name>Zurückstellen bis Termin</name>
  </names>
  <content_class type="TextChoose">
    <text key="">Nein</text>
    <text key="600">Ja, bis 10 Minuten nach Termin</text>
    <text key="300">Ja, bis 5 Minuten nach Termin</text>
    <text key="120">Ja, bis 2 Minuten nach Termin</text>
    <text key="60">Ja, bis 1 Minute nach Termin</text>
    <text key="0">Ja, bis Terminzeit</text>
    <text key="-60">Ja, bis 1 Minute vor Termin</text>
    <text key="-120">Ja, bis 2 Minuten vor Termin</text>
    <text key="-300">Ja, bis 5 Minuten vor Termin</text>
    <text key="-600">Ja, bis 10 Minuten vor Termin</text>
  </content_class>
</field>
<field key="lt">
  <names>
    <name>Zu spät ab</name>
  </names>
    <content_class type="TextChoose">
    <text key="">Nie</text>
    <text key="0">Sofort</text>
    <text key="1">1 min</text>
    <text key="5">5 min</text>
    <text key="10">10 min</text>
    <text key="15">15 min</text>
    <text key="30">30 min</text>
    <text key="60">60 min</text>
  </content_class>
</field>
<field key="apm">
  <names>
    <name>Anmelden mit"</name>
  </names>
  <content_class type="TextChoose">
    <!-- flags
      1: qr-code
      2: id
      4: forgot
    -->
    <text key="1">QR-Code</text>
    <text key="5">QR-Code und "QR-Code vergessen"</text>
    <text key="2">Terminnummer</text>
    <text key="3">QR-Code und Terminnummer</text>
  </content_class>
</field>
<field key="fgq">
  <names>
    <name>Warteschlange "Vergessen"</name>
  </names>
  <content_class type="VocatisQueue"  />
</field>
<field key="fgc">
  <names>
    <name>Kategorie "Vergessen"</name>
  </names>
  <content_class type="VocatisCategories"/>
</field>
<field key="aei">
  <names>
    <name>Terminfehler Info</name>
  </names>
  <content_class type="TextFree" />
</field>
<field key="at">
  <names>
    <name>Termin Timeout</name>
  </names>
  <content_class type="TextChoose">
    <text key="0">Wie Leerlauf-Timeout</text>
    <text key="5">5 Sekunden</text>
    <text key="10">10 Sekunden</text>
    <text key="15">15 Sekunden</text>
    <text key="20">20 Sekunden</text>
    <text key="30">30 Sekunden</text>
    <text key="40">40 Sekunden</text>
  </content_class>
</field>
{{/inline}}

{{#if use_groups_config}}
  <style key="vocatis_multi_2019_groupconfig{{suffix}}" file="printer/index.cshtml"
         interactive="true" target="chromium" preview="printer/assets/img/preview.png">
    <names>
      <name culture="de">vocatis printer Gruppenkonfiguration {{option_name}} ({{version}})</name>
    </names>
    <fields>
      <field key="entry">
        <names>
          <name>Einstieg</name>
        </names>
        <content_class type="TextChoose">
          <text key="g">groups</text>
          <text key="s">printer and appointments</text>
        </content_class>
      </field>
      {{> commonFields}}
      {{> appointmentFields}}
      {{> timeoutField}}
    </fields>
  </style>
{{else}}
  <style key="vocatic_multi_2019{{suffix}}" file="printer/index.cshtml"
         interactive="true" target="chromium" preview="printer/assets/img/preview.png">
    <names>
      <name culture="de">vocatis printer {{option_name}} ({{version}})</name>
    </names>
    <fields>
      {{> commonFields}}
      {{> printerFields}}
      {{#if multilang}}
        {{> timeoutField}}
      {{/if}}
    </fields>
  </style>

  <style key="vocm19aponly{{suffix}}" file="printer/index.cshtml"
         interactive="true" target="chromium" preview="printer/assets/img/preview.png">
    <names>
      <name culture="de">vocatis appointments {{option_name}} ({{version}})</name>
    </names>
    <fields>
      {{> commonFields}}
      {{> appointmentFields}}
      {{#if multilang}}
        {{> timeoutField}}
      {{/if}}
    </fields>
  </style>

  <style key="vocm19ap{{suffix}}" file="printer/index.cshtml"
         interactive="true" target="chromium" preview="printer/assets/img/preview.png">
    <names>
      <name culture="de">vocatis printer and appointments {{option_name}} ({{version}})</name>
    </names>
    <fields>
      {{> commonFields}}
      {{> appointmentFields}}
      {{> printerFields}}
      {{> timeoutField}}
    </fields>
  </style>
{{/if}}

  <style key="vocatis_ticket_default"
    file="printer/index.cshtml"
    interactive="false" target="chromium">
    <names>
      <name culture="de">Vocatis Wartemarke (Standard) {{option_name}} ({{version}})</name>
    </names>
    <fields>
      <field key="id">
        <names>
          <name>Ticket ID</name>
        </names>
        <content_class type="TextFree"  />
      </field>
      <field key="ticketnumber">
        <names>
          <name>Ticketnummer</name>
        </names>
        <content_class type="TextFree"  />
      </field>
    </fields>
  </style>
</StyleList>