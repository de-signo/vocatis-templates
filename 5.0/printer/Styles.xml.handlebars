<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<StyleList xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://stollsoft.de/iSign/v2/Content/Forms/Styles">
{{#*inline "commonFields"}}
<field key="mode">
  <names>
    <name>Modus</name>
  </names>
    <content_class type="TextChoose">
    <text key="show">Nummer anzeigen</text>
    <text key="print">Nummer drucken</text>
  </content_class>
</field>
<field key="tracking_id">
  <names>
    <name>Tracking-ID</name>
  </names>
  <content_class type="TextFree" />
</field>
{{/inline}}

{{#*inline "printerFields"}}
{{#range 1 9}}
<field key="text{{this}}">
  <names>
    <name>Name {{this}}</name>
  </names>
  <content_class type="TextFree" />
</field>
<field key="queueid{{this}}">
  <names>
    <name>Warteschlange {{this}}</name>
  </names>
  <content_class type="VocatisQueue"  />
</field>
<field key="catid{{this}}">
    <names>
        <name>Kategorie {{this}}</name>
    </names>
    <content_class type="VocatisCategories"/>
</field>
{{/range}}
{{/inline}}

{{#*inline "appointmentFields"}}
{{#range 1 9}}
<field key="plan_name{{this}}">
  <names>
    <name>Terminplan {{this}}</name>
  </names>
  <content_class type="TextFree" />
</field>
<field key="queueid{{this}}_apt">
  <names>
    <name>Warteschlange Termin {{this}}</name>
  </names>
  <content_class type="VocatisQueue"  />
</field>
{{/range}}
<field key="catid_apt">
  <names>
    <name>Kategorie Termin</name>
  </names>
  <content_class type="VocatisCategories"/>
</field>
<field key="import">
  <names>
    <name>Termine</name>
  </names>
  <content_class type="DataSource" />
</field>
<field key="id">
  <names>
    <name>ID</name>
  </names>
  <content_class bind="Column" parentField="import" type="TextFree" />
</field>
<field key="date">
  <names>
    <name>Datum</name>
  </names>
  <content_class bind="Column" parentField="import" type="TextFree" />
</field>
<field key="time">
  <names>
    <name>Uhrzeit</name>
  </names>
  <content_class bind="Column" parentField="import" type="TextFree" />
</field>
<field key="name">
  <names>
    <name>Name</name>
  </names>
  <content_class bind="Column" parentField="import" type="TextFree" />
</field>
<field key="title">
  <names>
    <name>Anliegen</name>
  </names>
  <content_class bind="Column" parentField="import" type="TextFree" />
</field>
<field key="plan">
  <names>
    <name>Terminplan</name>
  </names>
  <content_class bind="Column" parentField="import" type="TextFree" />
</field>
{{/inline}}

  <style key="vocatic_multi_2019{{suffix}}" file="Printer.cshtml"
         interactive="true" target="Forms2" preview="preview.png">
    <names>
      <name culture="de">vocatis printer {{option_name}} ({{version}})</name>
    </names>
    <fields>
      {{> commonFields}}
      {{> printerFields}}
    </fields>
  </style>

  <style key="vocm19aponly{{suffix}}" file="P1_Appointment.cshtml"
         interactive="true" target="Forms2" preview="preview.png">
    <names>
      <name culture="de">vocatis appointments {{option_name}} ({{version}})</name>
    </names>
    <fields>
      {{> commonFields}}
      {{> appointmentFields}}
    </fields>
  </style>

  <style key="vocm19ap{{suffix}}" file="P0_EntrySelect.cshtml"
         interactive="true" target="Forms2" preview="preview.png">
    <names>
      <name culture="de">vocatis printer and appointments {{option_name}} ({{version}})</name>
    </names>
    <fields>
      {{> commonFields}}
      {{> appointmentFields}}
      {{> printerFields}}
    </fields>
  </style>

  <style key="vocatis_ticket_default"
    file="TicketDefault.cshtml"
    interactive="false" target="Forms2">
    <names>
      <name culture="de">Vocatis Wartemarke (Standard) {{option_name}} ({{version}})</name>
    </names>
    <fields>
      <field key="id">
        <names>
          <name>Ticket ID</name>
        </names>
        <content_class type="TextFree"  />
      </field>
      <field key="ticketnumber">
        <names>
          <name>Ticketnummer</name>
        </names>
        <content_class type="TextFree"  />
      </field>
    </fields>
  </style>
</StyleList>