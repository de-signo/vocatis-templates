{
  "$schema": "../../node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "printer": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "vpr",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "allowedCommonJsDependencies": ["qrcode"],
            "outputPath": {
              "base": "dist/printer"
            },
            "index": "src/index.html",
            "polyfills": ["src/polyfills.ts"],
            "tsConfig": "tsconfig.printer.json",
            "optimization": {
              "styles": {
                "inlineCritical": false
              }
            },
            "assets": [
              "src/config.json",
              "src/favicon.ico",
              "src/assets",
              "src/Styles.xml"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["../styles", "src"]
            },
            "styles": ["src/styles.scss"],
            "scripts": [],
            "browser": "src/main.ts"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "600kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4.5kb",
                  "maximumError": "5kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "printer:build:production"
            },
            "development": {
              "buildTarget": "printer:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    },
    "app": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "app_src",
      "prefix": "vpra",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": {
              "base": "dist/app"
            },
            "index": "app_src/index.html",
            "polyfills": ["app_src/polyfills.ts"],
            "tsConfig": "tsconfig.app.json",
            "optimization": {
              "styles": {
                "inlineCritical": false
              }
            },
            "assets": [
              "app_src/favicon.ico",
              "app_src/assets",
              "app_src/Styles.xml"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["../styles", "app_src"]
            },
            "styles": ["app_src/styles.scss"],
            "scripts": [],
            "browser": "app_src/main.ts"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "5kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "app_src/environments/environment.ts",
                  "with": "app_src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app:build:production"
            },
            "development": {
              "port": 4201,
              "buildTarget": "app:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  }
}
