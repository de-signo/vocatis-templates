{
  "$schema": "../../node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "printer": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "vpr",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/printer",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.printer.json",
            "optimization": {
              "styles": {
                "inlineCritical": false
              }
            },
            "assets": [
              "src/appointments.cshtml",
              "src/config.json",
              "src/groups.cshtml",
              "src/number.cshtml",
              "src/queues.cshtml",
              "src/favicon.ico",
              "src/assets",
              "src/Styles.xml"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["../styles", "src"]
            },
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "index": {
                "input": "src/index.cshtml",
                "output": "index.cshtml"
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "printer:build:production"
            },
            "development": {
              "browserTarget": "printer:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    },
    "app": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "app_src",
      "prefix": "vpra",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app",
            "index": "app_src/index.html",
            "main": "app_src/main.ts",
            "polyfills": "app_src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "optimization": {
              "styles": {
                "inlineCritical": false
              }
            },
            "assets": [
              "app_src/favicon.ico",
              "app_src/assets",
              "app_src/Styles.xml"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["../styles", "app_src"]
            },
            "styles": [
              "app_src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "index": {
                "input": "app_src/index.cshtml",
                "output": "index.cshtml"
              },
              "fileReplacements": [
                {
                  "replace": "app_src/environments/environment.ts",
                  "with": "app_src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            },
            "development": {
              "browserTarget": "app:build:development",
              "port": 4201
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  },
  "defaultProject": "printer"
}
