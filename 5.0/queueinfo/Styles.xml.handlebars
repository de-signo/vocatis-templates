<StyleList xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://stollsoft.de/iSign/v2/Content/Forms/Styles">
  <style key="vocatic_queueinfo_2019{{suffix}}" file="QueueInfo.cshtml" preview="preview.png"
         interactive="true" target="chromium">
    <names>
      <name culture="de">Vocatis Warteinformation {{option_name}} ({{version}})</name>
    </names>
    <fields>
      <field key="appurl">
        <names>
          <name>Termin-Url</name>
        </names>
        <content_class type="TextFree" />
      </field>
      {{#range 1 9}}
      <field key="text{{this}}">
        <names>
          <name>Name {{this}}</name>
        </names>
        <content_class type="TextFree" />
      </field>
      <field key="queueid{{this}}">
        <names>
          <name>Warteschlange {{this}}</name>
        </names>
        <content_class type="VocatisQueue"  />
      </field>
      {{/range}}
    </fields>
  </style>
</StyleList>