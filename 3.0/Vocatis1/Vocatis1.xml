<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<StyleList xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://stollsoft.de/iSign/v2/Content/Forms/Styles">


  <style key="vocatis_1_display"
          file="Vocatis1.ascx"
         interactive="false" target="Forms 3.0">
    <names>
      <name culture="de">Vocatis Warteliste</name>
    </names>
    <fields>
      <field key="source">
        <names>
          <name>Vocatis Datenquelle</name>
        </names>
        <content_class type="DataSource" />
      </field>
      <field key="number">
        <names>
          <name>Nummer (Spalte)</name>
        </names>
        <content_class bind="Column" parentField="source" type="TextFree" />

      </field>
      <field key="room">
        <names>
          <name>Raum (Spalte)</name>
        </names>
        <content_class bind="Column" parentField="source" type="TextFree" />

      </field>
     

    </fields>
  </style>
  <style key="vocatis_1_printer_top"
           file="Vocatis1PrinterTop.ascx"
          interactive="true"  target="Info 3.0">
    <names>
      <name culture="de">Vocatis Wartemarkenspender</name>
    </names>
    <fields>
      <field key="views">
        <names>
          <name>Ansichten</name>
        </names>
        <content_class type="Views" />
      </field>
    </fields>
  </style>
  <style key="vocatis_1_printer_display"
            file="Vocatis1PrinterDisplay.ascx"
           interactive="true">
    <names>
      <name culture="de">Vocatis Wartemarken Monitoransicht</name>
    </names>
    <parentStyle>vocatis_1_printer_top</parentStyle>
    <fields>
     
    </fields>
  </style>
  <style key="vocatis_1_printer_ticket"
  file="Vocatis1PrinterTicket.ascx"
 interactive="false">
    <names>
      <name culture="de">Vocatis Wartemarke</name>
    </names>
    <parentStyle>vocatis_1_printer_top</parentStyle>
    <fields>
      <field key="view_key">
        <names>
          <name>Ansichtsname</name>
        </names>
      </field>
      <field key="queueid">
        <names>
          <name>Warteschlangen ID</name>
        </names>
        <content_class type="VocatisQueue"  />
      </field>
    </fields>
  </style>
  <style key="vocatis_ticket_default"
file="VocatisTicketDefault.ascx"
 interactive="false">
    <names>
      <name culture="de">Vocatis Wartemarke (Standard)</name>
    </names>
    <fields>
      <field key="ticketnumber">
        <names>
          <name>Ticketnummer</name>
        </names>
        <content_class type="TextFree"  />
      </field>
    </fields>
  </style>
</StyleList>