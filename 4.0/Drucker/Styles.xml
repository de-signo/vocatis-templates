<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<StyleList xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://stollsoft.de/iSign/v2/Content/Forms/Styles">
  <style key="vocatis_1r_printer"
           file="Printer.cshtml"
          interactive="true" target="Forms2">
    <names>
      <name culture="de">Vocatis Wartemarkenspender</name>
    </names>
    <fields>
       <field key="queueid">
        <names>
          <name>Warteschlange</name>
        </names>
        <content_class type="VocatisQueue"  />
      </field>
      <field key="catid">
          <names>
              <name>Kategorie</name>
          </names>
          <content_class type="VocatisCategories"/>
      </field>
      <field key="text">
        <names>
          <name>Text</name>
        </names>
        <content_class type="TextFree"  />
      </field>
    </fields>
  </style>
  <style key="vocatis_ticket_default"
    file="TicketDefault.cshtml"
    interactive="false" target="Forms2">
    <names>
      <name culture="de">Vocatis Wartemarke (Standard)</name>
    </names>
    <fields>
      <field key="ticketnumber">
        <names>
          <name>Ticketnummer</name>
        </names>
        <content_class type="TextFree"  />
      </field>
    </fields>
  </style>
</StyleList>